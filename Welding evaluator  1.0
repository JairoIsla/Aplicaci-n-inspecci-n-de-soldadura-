# Función para obtener criterios comunes entre dos códigos para un defecto dado
def obtener_criterios_comunes(defecto, codigo_origen, codigo_destino):
    criterios_origen = set()
    criterios_destino = set()
    criterio_defecto_origen = datos_soldadura[defecto][codigo_origen]
    criterio_defecto_destino = datos_soldadura[defecto][codigo_destino]

    if 'General' in criterio_defecto_origen:
        criterios_origen.update(criterio_defecto_origen['General'].keys())
    else:
        for key in criterio_defecto_origen.keys():
            if key not in ['Referencia', 'Descripcion']:
                criterios_origen.update(criterio_defecto_origen[key].keys())

    if 'General' in criterio_defecto_destino:
        criterios_destino.update(criterio_defecto_destino['General'].keys())
    else:
        for key in criterio_defecto_destino.keys():
            if key not in ['Referencia', 'Descripcion']:
                criterios_destino.update(criterio_defecto_destino[key].keys())

    criterios_comunes = criterios_origen.intersection(criterios_destino)
    return list(criterios_comunes)

# Función para evaluar si un defecto cumple con las normas de un código específico
def evaluar_defecto(tipo_defecto, codigo, estructura, carga, elemento, dimensiones, espesor_material, revaluar=False, codigo_origen=None):
    resultado = {}
    criterio_defecto = datos_soldadura.get(tipo_defecto, {}).get(codigo)

    if not criterio_defecto:
        resultado["cumple"] = False
        resultado["mensaje"] = f"No se encontró información para el defecto '{tipo_defecto}' en el código '{codigo}'."
        return resultado

    # Asegurarse de que dimensiones no es None
    if dimensiones is None:
        dimensiones = {}

    if 'General' in criterio_defecto:
        criterio_especifico = criterio_defecto['General']
    elif estructura and carga and elemento:
        try:
            criterio_especifico = criterio_defecto[estructura][carga][elemento]
        except KeyError:
            resultado["cumple"] = False
            resultado["mensaje"] = "No se encontró el criterio específico para la evaluación."
            return resultado
    else:
        resultado["cumple"] = False
        resultado["mensaje"] = "Faltan datos para la evaluación (estructura, carga, elemento)."
        return resultado

    if revaluar and codigo_origen:
        criterios_comunes = obtener_criterios_comunes(tipo_defecto, codigo_origen, codigo)
        dimensiones = {k: v for k, v in dimensiones.items() if k in criterios_comunes}

    if "permitido" in criterio_especifico:
        if not criterio_especifico["permitido"]:
            resultado["cumple"] = False
            resultado["mensaje"] = f"El defecto de {tipo_defecto.lower()} no es permitido."
            resultado["referencia"] = criterio_defecto.get("Referencia", "No disponible")
            resultado["descripcion"] = criterio_defecto.get("Descripcion", "No disponible")
        else:
            resultado["cumple"] = True
            resultado["mensaje"] = f"El defecto de {tipo_defecto.lower()} es permitido bajo ciertas condiciones."
            resultado["referencia"] = criterio_defecto.get("Referencia", "No disponible")
            resultado["descripcion"] = criterio_defecto.get("Descripcion", "No disponible")
    else:
        for dimension, valor in dimensiones.items():
            criterio_valor = criterio_especifico.get(dimension)
            if criterio_valor is None:
                continue  # No hay criterio para esta dimensión
            if valor > criterio_valor:
                resultado["cumple"] = False
                resultado["mensaje"] = f"El {dimension} de {tipo_defecto.lower()} excede el máximo permitido."
                resultado["referencia"] = criterio_defecto.get("Referencia", "No disponible")
                resultado["descripcion"] = criterio_defecto.get("Descripcion", "No disponible")
                resultado["rango"] = f"{criterio_valor}"
                resultado["valor_proporcionado"] = {k: v for k, v in dimensiones.items()}
                break
        else:
            resultado["cumple"] = True
            resultado["mensaje"] = f"El {tipo_defecto.lower()} está dentro de los límites permitidos."
            resultado["referencia"] = criterio_defecto.get("Referencia", "No disponible")
            resultado["descripcion"] = criterio_defecto.get("Descripcion", "No disponible")
            resultado["rango"] = {k: f"{criterio_especifico.get(k, 'No especificado')}" for k in dimensiones.keys()}
            resultado["valor_proporcionado"] = {k: v for k, v in dimensiones.items()}

    return resultado

# Función para capturar las dimensiones necesarias
def capturar_dimensiones(tipo_defecto, codigo, estructura, carga, elemento, espesor_material):
    dimensiones = {}
    criterio_defecto = datos_soldadura[tipo_defecto][codigo]

    if 'General' in criterio_defecto:
        criterios_dimensiones = criterio_defecto['General'].keys()
    elif estructura and carga and elemento:
        criterios_dimensiones = criterio_defecto[estructura][carga][elemento].keys()
    else:
        criterios_dimensiones = []

    for dimension in criterios_dimensiones:
        if dimension not in ['permitido', 'Referencia', 'Descripcion']:
            while True:
                try:
                    valor = float(input(f"Ingrese el valor de {dimension} (): "))
                    dimensiones[dimension] = valor
                    break
                except ValueError:
                    print(f"\nEntrada inválida para {dimension}. Por favor, ingrese un valor numérico.")
    return dimensiones

