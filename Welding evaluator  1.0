# Función para convertir unidades
def convertir_unidades(categoria, unidad_origen, unidad_destino, valor):
    try:
        return conversiones[categoria][unidad_origen][unidad_destino](valor)
    except KeyError:
        print("\nConversión no disponible entre las unidades seleccionadas.")
        return None

# Función para calcular potencia eléctrica
def calcular_potencia_electrica(volts, amps, factor_potencia=1):
    return volts * amps * factor_potencia

