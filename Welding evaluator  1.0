# Función para calcular la garganta teórica de un filete con lados diferentes
def calcular_garganta_teorica(tamano_lado1, tamano_lado2):
    hipotenusa = math.sqrt(tamano_lado1**2 + tamano_lado2**2)
    garganta = (tamano_lado1 * tamano_lado2) / hipotenusa
    return garganta

# Función para determinar el tamaño mínimo de filete según el espesor del material
def determinar_tamano_minimo_filete(espesor_material):
    if espesor_material <= 6:
        return 3
    elif espesor_material <= 12:
        return 5
    else:
        return 6

